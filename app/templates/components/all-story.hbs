<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <img src="{{story.urlToImage}}">
        <span class="card-title">{{story.title}}</span>
      </div>
      <div class="card-content">
        {{story.description}}
      </div>
      <div class="card-action">
        <a href="{{story.url}}">View Story</a>
      </div>
    </div>
  </div>
</div>
